            <text:p text:style-name="P5">Anmeldung an das Handelsregisteramt</text:p>
            <text:p text:style-name="P4"/>
            <text:list xml:id="list595537471" text:style-name="WWNum6">
                <text:list-item>
                    <text:p text:style-name="P7">
                        <text:span text:style-name="Absatz-Standardschriftart">
                            <text:span text:style-name="T7">Zur Eintragung in das Handelsregister wird folgende
                                Neueintragung angemeldet:
                            </text:span>
                        </text:span>
                    </text:p>
                </text:list-item>
                <text:list-item>
                    <text:p text:style-name="P9"/>
                </text:list-item>
                <text:list-item>
                    <text:p text:style-name="P9"/>
                </text:list-item>
                <text:list-item>
                    <text:p text:style-name="P10">Firma</text:p>
